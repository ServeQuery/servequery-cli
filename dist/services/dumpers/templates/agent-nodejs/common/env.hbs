SERVEQUERY_AUTH_SECRET={{ servequeryAuthSecret }}
SERVEQUERY_ENV_SECRET={{ servequeryEnvSecret }}

{{#if servequeryServerUrl}}
SERVEQUERY_SERVER_URL={{ servequeryServerUrl }}
# This should be removed in production environment.
NODE_TLS_REJECT_UNAUTHORIZED=0

{{/if}}
APPLICATION_PORT={{ appPort }}

DATABASE_URL={{ dbUrl }}
{{#unless isMongoose}}
DATABASE_SSL_MODE={{ dbSslMode }}
{{/unless}}
{{#if dbSchema }}
DATABASE_SCHEMA={{ dbSchema }}
{{/if}}
{{#if hasDockerDbUrl}}
DOCKER_DATABASE_URL={{ dockerDbUrl }}
{{/if}}

NODE_ENV=development
